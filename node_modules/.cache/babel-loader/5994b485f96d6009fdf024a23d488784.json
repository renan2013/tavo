=\"flex items-center ml-1\">\n                <vs-checkbox v-model=\"isSelectedMail\" @click.stop class=\"vs-checkbox-small ml-0 mr-1\"></vs-checkbox>\n                \n            </div>\n            <div class=\"mail__message truncate ml-3\">\n                <div class=\"flex border d-theme-dark-bg items-center\">\n                <div class=\"vx-col md:w-1/3 w-full\">\n                 <vs-button line-origin=\"left\" color=\"dark\" type=\"line\"  @click=\"atieneTarea()\" >\n                   <span>Proceso#:{{ proceso.NUM_HILO  }}</span>\n                   </vs-button>\n                </div>   \n                     <vs-button radius color=\"dark\" type=\"line\" icon=\"person_add\"><span> Usuario que Reporto:</span>\n                       <span> {{ proceso.INICIADOR  }}</span>\n                   </vs-button>\n                 \n                 <div class=\"mail-item__meta flex items-center\">\n                   <template>\n                      \n                       <flat-pickr :config=\"configdateTimePicker\" v-model=\"proceso.ASIGNACION\" placeholder=\"Date Time\" />\n                     \n                   </template>\n                   <span>Asignado:{{ proceso.ASIGNACION }}</span>\n                </div>\n                <div class=\"vx-col md:w-1/4 w-full\">\n                  \n                </div>  \n                 \n\n            </div>     \n            </div>\n        </div>\n        <!-- /MAIL ROW 2 -->\n        <!-- MAIL ROW 1 : META -->\n        <div class=\"flex w-full\">\n          \n            <div class=\"flex w-full justify-between items-start\">\n                <div class=\"mail__details\">\n                     \n\n                       <vs-avatar color=\"dark\" icon=\"filter_vintage\"/>\n                     \n                     <vs-button line-origin=\"left\" color=\"dark\" type=\"line\"  @click=\"atieneTarea()\" >{{ proceso.DESCRIPCION}}</vs-button>\n                    \n                    \n                </div>\n\n                <div class=\"mail-item__meta flex items-center\">\n                    <span>Estado:{{ proceso.ESTADO }}</span>\n                </div>\n            </div>\n\n\n\n        </div>\n        <!-- /MAIL ROW 1 -->\n\n    </div>\n</template>\n\n<script>\n\n// NO USAR SI NO HAY FECHAS\nimport flatPickr from 'vue-flatpickr-component';\nimport 'flatpickr/dist/flatpickr.css';\n\nexport default {\n  props: {\n    proceso: {\n      type: Object,\n      required: true\n    },\n    isSelected: {\n      type: Boolean,\n      required: true\n    }\n  },\n  data () {\n    return {\n      isSelectedMail: this.isSelected\n    }\n  },\n  watch: {\n    isSelected (value) {\n      this.isSelectedMail = value\n    },\n    isSelectedMail (val) {\n     this.atieneTarea();\n    }\n  }, \n  methods: {\n    atieneTarea() {\n      //LLAMA BUS \n      let datosProceso=[{\n          hilo:this.proceso.NUM_HILO,\n          hiloActivo:this.proceso.ID,\n          dpto:this.proceso.NUM_DPTO\n\n      }]\n     this.$emit('callTareaAtender',  datosProceso)\n     \n    },\n    toggleIsStarred () {\n      const payload = { mailId: this.proceso.ID, value: \"A\"}\n      this.$store.dispatch('StoreBpm//toggleIsMailStarred', payload)\n    }\n  }\n}\n\n</script>\n\n"],"sourceRoot":"src/divisoft/divisoftBpm"}]}