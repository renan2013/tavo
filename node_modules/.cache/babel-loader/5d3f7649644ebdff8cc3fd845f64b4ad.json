ata-list-add-new pt-4 pb-6  vs-bpmedit\">\n      <vx-input-group class=\"mb-base\">  \n   \n            <div class=\"vx-row\">\n              \n                <div class=\"vx-row\">\n\n                    \n\n                    <div class=\"vx-col w-1/2  vs-bpmedit\" :key=\"indextr\" v-for=\"(item,indextr ) in datosTarea\"  >\n                       <span>{{item.NOMBRE_CAMPO}}</span>\n                        <vs-input v-if=\"item.TIPO==1\" \n                           \n                               size=\"large\" \n                               v-validate=\"'required'\" \n                               placeholder=item.PLACEHOLDER\n                               \n                               v-model=\"item.CAMPO_VALOR\" \n                               :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                               class=\"mt-5 w-full\" \n                               />\n\n                        <vs-input v-if=\"item.TIPO==2\" \n                         \n                               size=\"large\" \n                               v-validate=\"'required'\" \n                               :placeholder=item.PLACEHOLDER\n                               \n                               v-model=\"item.CAMPO_VALOR\" \n                                :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                               class=\"mt-5 w-full\" \n                               />       \n\n                        <vs-input v-if=\"item.TIPO==3\" \n                               size=\"large\" \n                               v-validate=\"'required'\" \n                               :placeholder=item.PLACEHOLDER\n                               \n                               v-model=\"item.CAMPO_VALOR\" \n                                :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                               class=\"mt-5 w-full\" \n                               />      \n                        \n                        \n                        <template> \n                            <flat-pickr v-if=\"item.TIPO==4\" \n                            :config=\"configdateTimePicker\"\n                             \n                             v-model=\"item.CAMPO_VALOR\" \n                             :placeholder=item.PLACEHOLDER \n                              :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                             \n                             />\n                        </template>\n\n                         <vs-textarea v-if=\"item.TIPO==5\"\n                         size=\"large\"\n                          name=\"item.NOMBRE_INTERNO\" \n                           \n                           :placeholder=item.PLACEHOLDER \n                            v-model=\"item.CAMPO_VALOR\" \n                             :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                          />\n \n                        <!--  RADIO GROUP  -->\n\n                        <li  v-if=\"item.TIPO==6\" \n                         \n                           :key=\"index2\"  \n                           v-for=\"(itemVal,index2) in item.VALOR_DEFAULT\" \n                           \n                           >\n                            <vs-radio  v-model=\"item.CAMPO_VALOR\"  \n                              :vs-value=\"itemVal.valor\">{{itemVal.label }}</vs-radio>\n                       </li>\n                         \n\n                        <!--  lista de Valores  Select   -->\n\n                        <vs-select    v-if=\"item.TIPO==7   ||item.TIPO==8\" \n                        \n                        v-model=\"item.VALOR\" \n                        \n                        class=\"w-full select-large\"\n                         :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                        >\n                            <vs-select-item \n                            :key=\"index2\" \n                            v-model=\"item.CAMPO_VALOR\" \n                            :value=\"itemVal.valor\" \n                            :text=\"itemVal.label\" \n                            v-for=\"(itemVal,index2)  in item.VALOR_DEFAULT\" \n                            class=\"w-full\" />\n                        </vs-select>\n\n                        <vs-input \n                        v-if=\"item.TIPO==12\" \n                           \n                        size=\"large\" \n                        v-validate=\"'email'\" \n                        v-model=\"item.CAMPO_VALOR\"  \n                        :placeholder=item.PLACEHOLDER \n                        name=\"item.NOMBRE_INTERNO\" \n                         class=\"mt-5 w-full\" \n                          :disabled=\"item.IND_SOLOLECTURA=='true'\"  \n                         />\n\n\n                        <template>\n                            <div v-if=\"item.TIPO==44\" \n                            class=\"centerx\">\n                                <vs-input-number \n                               v-model=\"item.CAMPO_VALOR\"\n                               \n                                :disabled=\"item.IND_SOLOLECTURA=='true'\"   \n                                />\n\n                            </div>\n                        </template>\n                        <template if=\"item.TIPO==28\" >\n                           <!--  para llamar a ruta de  Formulario vue -->\n                             <vs-button line-origin=\"left\" color=\"dark\" type=\"line\"  @click=\"llamaRutaVue(item.CAMPO_VALOR)\" >{{ item.NOMBRE_CAMPO }}</vs-button>\n                        </template>   \n \n\n                    </div>\n \n\n                    \n\n                </div>\n            </div>\n\n             <div>\n                  <button type=\"submit\" class=\"btn btn-success\" @click=\"GuardaRegistro()\">\n                Salvar Datos\n            </button>\n             </div>\n             <div>\n<button type=\"submit\" class=\"btn btn-success\" @click=\"regresa()\">\n                Regresar\n            </button>\n             </div>\n </vx-input-group>\n    </VuePerfectScrollbar>\n  </vx-card>\n</template> \n\n\n\n\n<script>\nimport {mapState} from \"vuex\"; //, mapMutations, mapActions \n\nimport {eventBus} from \"@/main\"; // para que se hablen los componentes  \n\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar';\n\n// mensajes de error personalizados\n import { Validator } from \"vee-validate\";\n\nimport * as datosJs from \"./js/dbpm01.js\";\nimport * as divilib from \"@/divisoftlibs/utilDivisoftJS.js\";\n// NO USAR SI NO HAY FECHAS\nimport flatPickr from \"vue-flatpickr-component\";\nimport \"flatpickr/dist/flatpickr.min.css\";\nimport {Spanish} from \"flatpickr/dist/l10n/es.js\";\n\nimport moduleDbpm from \"@/store/dBpm/moduleDbpm.js\";\n\n// FIN IMPORT POR FECHAS   \nconst dict = datosJs.mensajesValidacion;\n// registra los mensajes personalizados\nValidator.localize(\"en\", dict);\n\nexport default {  \n    created() {\n    //registro el modulo para que este disponible en Store\n   this.$store.registerModule(\"StoreBpm\", moduleDbpm);\n\n    let datosRelevante={\n            NUM_HILO: 0,\n            ID_VARIABLE: \"MONTO\",\n            VALOR: \"5000\",\n            VALOR_LARGO: \"NO APLICA\"\n        };\n\n\n    this.$store.commit(\"StoreBpm/SET_RELEVANTEVALUE\",  datosRelevante); // Update Mail Filter\n\n   datosRelevante.ID_VARIABLE=\"NUEVORELEVANTE\";\n   this.$store.commit(\"StoreBpm/ADD_RELEVANTEVALUE\",  datosRelevante); // Update Mail Filter\n\n\n    datosRelevante.ID_VARIABLE=\"NOMBRE\";\n    //this.$store.commit(\"StoreBpm/DELETE_RELEVANTEVALUE\",  datosRelevante); // Update Mail Filter \n     },\n  \n   props: {\n\t  datosTarea: {  //registros del prompt \n\t\t\t// header Es recibido del padre\n\t\t\ttype: Array,\n      required: true\n      }\n    }, \n \n  components: {\n      VuePerfectScrollbar, \n      flatPickr \n  }, \n    watch: {\n        // SI HAY PROMPT\n        ///XXXXXXXXX  \n        lastDbmsgCode: function () {\n            if (this.lastDbmsgCode != 0) {\n                this.$vs.notify({\n                    title: \"Servicio Restfull\",\n                    time: 6000,\n                    text: this.lastDbmsg,\n                    color: \"warning\"\n                });\n            }\n        },\n        currentRecord: function () {\n            if (this.currentRecord.COD_CLIENTE > 0)\n                this.cargaRegistroActual(this.currentRecord);\n        },\n\n    },\n    data() {\n        return {\n            messageError: \"Sin Errores\",\n            \n            ///////////////// FECHAS FECHAS\n            configdateTimePicker: {\n                locale: Spanish\n            },\n            configFechaHoraPicker2: divilib.FechaHoraformat,\n            configHoraPicker2: divilib.Horaformat,\n            valorAutocomplete: \"\",\n            // CAMPOS DE LISTA  SI LOS HAHAY \n            //////////////// CAMPOS DEL MANTENIMIENTO  LLAMAR A UN DEFAULT\n\n            //Datos  Iniciales  \n            actualizo: false,\n            iniciado: false,\n            defaultdatos: [],\n            // SI No hay lista de Valores puede quitarse\n            indicadorPrompt: 0,\n            rowselectionprompt: [],\n            datosPrompt: [{\n                titulo: \"\"\n            }],\n\n            //Formulario \n            counterDanger: false,\n            origenUrl: \"J\",\n            activaPrompt: false,\n            //scrollbar\n            settings: {\n                maxScrollbarLength: 60\n            },\n\n            parametros: [],\n            locallastheader: \"\", //guarda el ultimo header de este objeto\n            locallaststrParametros: \"\", //guarda el ultimo  parametros de este objeto\n            locallaststrFilas: \"\", //guarda el ultimo filas de este objeto\n            paramtxt: \"\",\n            filastxt: \"\",\n            header: \"\"\n        };\n    },\n\n    computed: {\n        ...mapState([\"dsoaModel\", \"rowsForm\", \"DataCMS\", \"tableKey\", \"urlOrigen\"]),\n        getProfile() {\n            return this.$store.state.dsoaLogin.profile[0];\n        },\n        getDataRelevante () {\n              return this.$store.getters['StoreBpm/buscaRelevante'](\"NOMBRE\")\n          },\n\n       getRelevantes() {\n             return this.$store.state.StoreBpm.datosRelevantes;\n            \n        },\n        currentRecord() {\n            return this.$store.state.rowsForm[0];\n        },\n        CMSDataRespuesta() {\n            return this.$store.state.DataCMS;\n        },\n        lastDbmsg() {\n            return this.$store.state.lastBdmsage;\n        },\n        procesando() {\n            return this.$store.state.estadoProcesando;\n        },\n        lastDbmsgCode() {\n            return this.$store.state.lastBdmsgcode;\n        },\n        estadoProcesando() {\n            return this.$store.state.estadoProcesando;\n        }\n    },\n\n    methods: \n        {\n        // FIN DE  METODO QUE CARGA LOS DATOS DEL FILTRO PARA LOS PROMPT\n        llamaRutaVue(routename) {\n              this.$router.push(routename) \n           // this.$parent.activaEditSimple = false;\n        },\n        \n        regresa() {\n           \n            this.$emit('closeSidebar');\n           // this.$parent.activaEditSimple = false;\n        },\n\n        //FORMULARIO\n\n         \n\n        // FIN DE VALIDA PASOS ESTO SI ES UN FORMULARIO DE PASOS \n\n        addFilas: function () {\n            var filas = [];\n\n            filas.push({\n                NOMBRE: \"COD_CIA\",\n                VALOR1: this.COD_CIA,\n                VALOR2: \"\",\n                CDATA: \"0\"\n            });\n            filas.push({\n                NOMBRE: \"NOM_CIA\",\n                VALOR1: this.NOM_CIA,\n                VALOR2: \"\",\n                CDATA: \"0\"\n            });\n            this.filastxt = divilib.filaArraytoStrintg(filas, \"S\");\n        },\n\n        //TRAE DATOS PARA MOSTRAR EN FORMULARIO\n\n        formSubmitted: function () {\n            this.GuardaRegistro();\n        },\n        GuardaRegistro: function () {\n            //HEADER\n            var header = {\n                PAGINA: \"1\"\n            };\n            // \n        }, // fin de  metodo\n\n        // CARGA DE una variable Bd a DOM\n        cargaRegistroActual: function (datosRow) {\n\n        },\n        //cargaRegistroActual funcion completa   \n\n    } // fin de  methods\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.add-new-data-sidebar {\n  /deep/ .vs-sidebar--background {\n    z-index: 52010;\n  }\n\n  /deep/ .vs-bpmedit {\n    z-index: 52010;\n   padding-left: 10%;\n    max-width: 90vw;\n  }\n}\n</style>\n\n"],"sourceRoot":"src/divisoft/divisoftBpm"}]}