   <div class=\"px-6 pb-2 flex flex-col\">\n\n                <!-- inbox -->\n                <router-link tag=\"span\" :to=\"`${baseUrl}/U`\" class=\"flex justify-between items-center cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"MailIcon\" :svgClasses=\"[{'text-primary stroke-current': bpmFilter == 'U'}, 'h-6 w-6']\"></feather-icon>\n                        <span class=\"text-lg ml-3\">Mis tareas</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n                 <router-link tag=\"span\" :to=\"`${baseUrl}/P`\" class=\"flex justify-between items-center mt-4 cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"Edit2Icon\" ></feather-icon>\n                        <span class=\"text-lg ml-3\">Tareas Nuevas a Tomar</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n \n\n                <!-- draft -->\n                <router-link tag=\"span\" :to=\"`${baseUrl}/R`\" class=\"flex justify-between items-center mt-4 cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"Edit2Icon\" ></feather-icon>\n                        <span class=\"text-lg ml-3\">Tareas con Retraso</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n\n                <router-link tag=\"span\" :to=\"`${baseUrl}/H`\" class=\"flex justify-between items-center mt-4 cursor-pointer\" >\n                    <div