plate >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n\n                <router-link tag=\"span\" :to=\"`${baseUrl}/H`\" class=\"flex justify-between items-center mt-4 cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"Edit2Icon\" ></feather-icon>\n                        <span class=\"text-lg ml-3\">Terminados</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n                 \n\n                <!-- spam -->\n                <router-link tag=\"span\" :to=\"`${baseUrl}/T`\" class=\"flex items-center justify-between items-center mt-4 cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"InfoIcon\" ></feather-icon>\n                        <span class=\"text-lg ml-3\">Proximas</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n\n                <router-link tag=\"span\" :to=\"`${baseUrl}/O`\" class=\"flex items-center justify-between items-center mt-4 cursor-pointer\" >\n                    <div class=\"flex items-center mb-2\">\n                        <feather-icon icon=\"InfoIcon\"></feather-icon>\n                        <span class=\"text-lg ml-3\">Donde he Participado</span>\n                    </div>\n                    <template >\n                      <vs-chip class=\"number\" color=\"primary\" >5</vs-chip>\n                    </template>\n                </router-link>\n \n            </div>\n            <vs-divider ></vs-divider>\n            \n        </component>\n    </div>\n</template>\n