 /* UnComment below lines for enabling true flow if deleting user */\n      // this.$store.dispatch(\"userManagement/removeRecord\", this.user_data.id)\n      //   .then(()   => { this.$router.push({name:'app-user-list'}); this.showDeleteSuccess() })\n      //   .catch(err => { console.error(err)       })\n    },\n\n    showDeleteSuccess() {\n      this.$vs.notify({\n        color: \"success\",\n        title: \"User Deleted\",\n        text: \"The selected user was successfully deleted\",\n      });\n    },\n  },\n};\n",{"version":3,"sources":["UserView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgHA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"UserView.vue","sourceRoot":"src/views/apps/user","sourcesContent":["<!-- =========================================================================================\n  File Name: UserView.vue\n  Description: User View page\n  ----------------------------------------------------------------------------------------\n  Item Name: Vuexy - Vuejs, HTML & Laravel Admin Dashboard Template\n  Author: Pixinvent\n  Author URL: http://www.themeforest.net/user/pixinvent\n========================================================================================== -->\n<!-- =========================================================================================\n  File Name: UserView.vue\n  Description: User View page\n  ----------------------------------------------------------------------------------------\n  Item Name: Vuexy - Vuejs, HTML & Laravel Admin Dashboard Template\n  Author: Pixinvent\n  Author URL: http://www.themeforest.net/user/pixinvent\n========================================================================================== -->\n\n<template>\n  <div id=\"page-user-view\">\n    <!--vs-alert\n      color=\"danger\"\n      title=\"User Not Found\"\n      :active.sync=\"user_not_found\"\n    >\n      <span>User record with id: {{ $route.params.userId }} not found. </span>\n      <span>\n        <span>Check </span\n        ><router-link\n          :to=\"{ name: 'page-user-list' }\"\n          class=\"text-inherit underline\"\n          >All Users</router-link\n        >v-if=\"user_data\"\n      </span>\n    </vs-alert-->\n\n    <div id=\"user-data\">\n      <vx-card title=\"Perfil\" class=\"mb-base\">\n        <!-- Avatar -->\n        <div class=\"vx-row\">\n          <!-- Avatar Col -->\n          <div class=\"vx-col\" id=\"avatar-col\">\n            <div class=\"img-container mb-4\">\n              <img\n                src=\"@/assets/images/pages/rosebox.jpg\"\n                class=\"rounded w-full\"\n              />\n            </div>\n          </div>\n\n          <!-- Information - Col 1 -->\n          <div class=\"vx-col flex-1\" id=\"account-info-col-1\">\n            <table>\n              <tr>\n                <td class=\"font-semibold\">Usuario</td>\n                <td>{{ getProfile.Username }}</td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Nombre</td>\n                <td>{{ getProfile.Nombre }}</td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Email</td>\n                <td>{{ getProfile.Email }}</td>\n              </tr>\n            </table>\n          </div>\n          <!-- /Information - Col 1 -->\n\n          <!-- Information - Col 2 -->\n          <div class=\"vx-col flex-1\" id=\"account-info-col-2\">\n            <table>\n              <tr>\n                <td class=\"font-semibold\">Id_Usuario</td>\n                <td>{{ getProfile.num_user }}</td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Rol</td>\n                <td>{{ getProfile.Role }}</td>\n              </tr>\n              <tr>\n                <td class=\"font-semibold\">Company</td>\n                <td>{{ getProfile.Nombre_Agencia }}</td>\n              </tr>\n            </table>\n          </div>\n          <!-- /Information - Col 2 -->\n          <!--div class=\"vx-col w-full flex\" id=\"account-manage-buttons\">\n            <vs-button\n              icon-pack=\"feather\"\n              icon=\"icon-edit\"\n              class=\"mr-4\"\n              :to=\"{\n                name: 'app-user-edit',\n                params: { userId: $route.params.userId },\n              }\"\n              >Editar</vs-button\n            >\n            <vs-button\n              type=\"border\"\n              color=\"danger\"\n              icon-pack=\"feather\"\n              icon=\"icon-trash\"\n              @click=\"confirmDeleteRecord\"\n              >Delete</vs-button>\n          </div-->\n        </div>\n      </vx-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport moduleUserManagement from \"@/store/user-management/moduleUserManagement.js\";\nimport { mapState } from \"vuex\";\n\nexport default {\n  computed: {\n    getProfile() {\n      return this.$store.state.dsoaLogin.profile[0];\n    },\n  },\n\n  methods: {\n    confirmDeleteRecord() {\n      this.$vs.dialog({\n        type: \"confirm\",\n        color: \"danger\",\n        title: \"Confirm Delete\",\n        text: `You are about to delete \"${this.user_data.username}\"`,\n        accept: this.deleteRecord,\n        acceptText: \"Delete\",\n      });\n    },\n\n    deleteRecord() {\n      /* Below two lines are just for demo purpose */\n      this.$router.push({ name: \"app-user-list\" });\n      this.showDeleteSuccess();\n\n      /* UnComment below lines for enabling true flow if deleting user */\n      // this.$store.dispatch(\"userManagement/removeRecord\", this.user_data.id)\n      //   .then(()   => { this.$router.push({name:'app-user-list'}); this.showDeleteSuccess() })\n      //   .catch(err => { console.error(err)       })\n    },\n\n    showDeleteSuccess() {\n      this.$vs.notify({\n        color: \"success\",\n        title: \"User Deleted\",\n        text: \"The selected user was successfully deleted\",\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#avatar-col {\n  width: 10rem;\n}\n\n#page-user-view {\n  table {\n    td {\n      vertical-align: top;\n      min-width: 140px;\n      padding-bottom: 0.8rem;\n      word-break: break-all;\n    }\n\n    &:not(.permissions-table) {\n      td {\n        @media screen and (max-width: 370px) {\n          display: block;\n        }\n      }\n    }\n  }\n}\n\n// #account-info-col-1 {\n//   // flex-grow: 1;\n//   width: 30rem !important;\n//   @media screen and (min-width:1200px) {\n//     & {\n//       flex-grow: unset !important;\n//     }\n//   }\n// }\n\n@media screen and (min-width: 1201px) and (max-width: 1211px),\n  only screen and (min-width: 636px) and (max-width: 991px) {\n  #account-info-col-1 {\n    width: calc(100% - 12rem) !important;\n  }\n\n  // #account-manage-buttons {\n  //   width: 12rem !important;\n  //   flex-direction: column;\n\n  //   > button {\n  //     margin-right: 0 !important;\n  //     margin-bottom: 1rem;\n  //   }\n  // }\n}\n</style>\n"]}]}